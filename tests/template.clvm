(mod (alice bob amount seconds person)
  (if (= alice person)
     (list ;; Originator can recover or spend until the coin is spent by other person.
      (c (q . 51) (c alice (c amount (q . ()))))
      )
     (list
      (c (q . 80) (c seconds (q . ())))
      (c (q . 51) (c bob (c amount (q . ()))))
      )
  )
)